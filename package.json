{
    "name": "nextapp",
    "version": "0.1.0",
    "private": true,
    "imports": {
        "#tailwind.css": "./src/app/styles/tailwind.css"
    },
    "prisma": {
        "schema": "src/database/schema",
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} -r tsconfig-paths/register src/database/seed/index.ts"
    },
    "scripts": {
        "dev": "rimraf -rf .next && next dev --turbopack",
        "build": "rimraf -rf .next && next build",
        "start": "next start",
        "lint": "pnpm lint:es && pnpm lint:style",
        "lint:es": "next lint --fix",
        "lint:style": "stylelint \"**/*.css\" --fix --cache --cache-location node_modules/.cache/stylelint/",
        "addsc": "pnpm dlx shadcn@latest add",
        "------------------ db command": "----",
        "dbg": "cross-env NODE_ENV=development prisma generate",
        "dbp": "cross-env NODE_ENV=development prisma db push",
        "dbpl": "cross-env NODE_ENV=development prisma db pull",
        "dbmc": "cross-env NODE_ENV=development prisma migrate dev --create-only --skip-generate",
        "dbm": "cross-env NODE_ENV=development prisma migrate dev --skip-seed",
        "dbmr": "cross-env NODE_ENV=development prisma migrate reset -f --skip-seed",
        "dbms": "cross-env NODE_ENV=development prisma migrate dev",
        "dbs": "prisma db seed",
        "dbmd": "cross-env NODE_ENV=production prisma migrate deploy",
        "dbo": "prisma studio",
        "postinstall": "pnpm run dbm && pnpm run dbs && pnpm run dbg"
    },
    "dependencies": {
        "@ant-design/cssinjs": "^1.23.0",
        "@ant-design/nextjs-registry": "^1.0.2",
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@faker-js/faker": "^9.9.0",
        "@hookform/resolvers": "^5.2.1",
        "@prisma/client": "6.14.0",
        "@radix-ui/react-alert-dialog": "^1.1.14",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@radix-ui/react-slot": "^1.2.3",
        "antd": "^5.25.4",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "deepmerge": "^4.3.1",
        "immer": "^10.1.1",
        "lodash": "^4.17.21",
        "lucide-react": "^0.511.0",
        "micromatch": "^4.0.8",
        "next": "15.3.3",
        "prisma-paginate": "^5.2.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.62.0",
        "react-use": "^17.6.0",
        "tailwind-merge": "^3.3.0",
        "tailwindcss-animate": "^1.0.7",
        "uuid": "^11.1.0",
        "zod": "^4.0.17",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^4.13.2",
        "@eslint-react/eslint-plugin": "^1.50.0",
        "@eslint/eslintrc": "^3.0.0",
        "@tailwindcss/postcss": "^4",
        "@types/lodash": "^4.17.17",
        "@types/micromatch": "^4.0.9",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/uuid": "^10.0.0",
        "autoprefixer": "^10.4.21",
        "cross-env": "^10.0.0",
        "eslint": "^9.0.0",
        "eslint-config-next": "15.3.3",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-perfectionist": "^4.13.0",
        "eslint-plugin-prettier": "^5.4.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-unused-imports": "^4.1.4",
        "postcss-mixins": "^12.0.0",
        "postcss-nesting": "^13.0.2",
        "prettier": "^3.5.3",
        "prisma": "^6.14.0",
        "rimraf": "^6.0.1",
        "stylelint": "^16.20.0",
        "stylelint-config-css-modules": "^4.4.0",
        "stylelint-config-recess-order": "^6.0.0",
        "stylelint-config-standard": "^38.0.0",
        "stylelint-prettier": "^5.0.3",
        "tailwindcss": "^4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tw-animate-css": "^1.3.5",
        "typescript": "^5",
        "utility-types": "^3.11.0"
    }
}
